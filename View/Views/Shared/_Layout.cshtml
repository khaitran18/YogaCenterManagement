<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="~/images/favicon.png" />
    <title>YOGAGURU</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/animate.css" rel="stylesheet">
    <link href="~/css/themify-icons.css" rel="stylesheet">
    <link href="~/modules/slick/slick.css" rel="stylesheet">
    <link href="~/modules/slick/slick-theme.css" rel="stylesheet">
    <link href="~/modules/magnific-popup/magnific-popup.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;1,300;1,400;1,500;1,600;1,700;1,800&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Open%20Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.7.1/css/all.min.css" integrity="sha512-3M00D/rn8n+2ZVXBO9Hib0GKNpkm8MSUU/e2VNthDyBYxKWG+BftNYYcuEjXlyrSO637tidzMBXfE7sQm0INUg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-144098545-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-144098545-1');
    </script>
</head>
<body>
    <!-- Page Preloader -->
    <div id="yg-page-loading" class="yg-pageloading">
        <div class="yg-pageloading-inner">
            <h1>YOGAGURU</h1>
        </div>
    </div>
    <!-- Mouse cursor -->
    <div class="mouse-cursor cursor-outer"></div>
    <div class="mouse-cursor cursor-inner"></div>
    <!-- Navigation Overlay -->
    <div class="yg-nav-overlay yg-nav-overlay-closed">
        <div class="yg-nav-overlay-inner">
            <div class="yg-nav-overlay-menu-wrapper">
                <div class="yg-nav-overlay-menu-wrapper-inner">
                    <ul class="yg-nav-overlay-main-nav">
                        <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                        @if (Context.Request.Cookies["AuthToken"] != null)
                        {
                            <li><a asp-controller="User" asp-action="Details">Your Profile</a></li>
                        }
                        else
                        {
                            <li><a asp-controller="Auth" asp-action="Login">Login/Signup</a></li>
                        }
                        <li><a class="active" asp-controller="Class" asp-action="Index">Classes</a></li>
                        <li><a href="schedule.html">Schedule</a></li>
                        @if (Context.Request.Cookies["Role"] == "Lecturer")
                        {
                            <li><a asp-controller="User" asp-action="Feedback">Feedback</a></li>
                        }
                        @if (Context.Request.Cookies["AuthToken"] != null)
                        {
                            @if (Context.Request.Cookies["Role"] == "User")
                            {
                                <li><a asp-controller="Class" asp-action="StudyingClasses">Your studying classes</a></li>
                                <li><a asp-controller="Class" asp-action="StudiedClasses">Your studied classes</a></li>
                            }
                            else if (Context.Request.Cookies["Role"] == "Lecturer")
                            {
                                <li><a asp-controller="Class" asp-action="TeachingClasses">Your teaching classes</a></li>
                                <li><a asp-controller="Class" asp-action="TeachingClasses">Your teaching classes</a></li>
                                <li><a asp-controller="Class" asp-action="AvailableDates">Your available dates</a></li>
                            }
                        }
                        <li><a asp-controller="Home" asp-action="AboutUs">About Us</a></li>
                        @if (Context.Request.Cookies["AuthToken"] != null)
                        {
                            <li><a asp-controller="Auth" asp-action="Logout">Logout</a></li>
                        }
                    </ul>
                    <a href="classes-page.html#" class="yg-nav-overlay-close" id="yg-nav-overlay-close">
                        <span></span>
                        <svg x="0px" y="0px" width="54px" height="54px" viewBox="0 0 54 54">
                            <circle fill="transparent" stroke="#000000" stroke-width="1" cx="27" cy="27" r="25" stroke-dasharray="157 157" stroke-dashoffset="157"></circle>
                        </svg>
                    </a>
                </div>
            </div>
        </div>
    </div>

    <div id="page-wrapper">
        <!-- Header start -->
        <header>
            <div class="yg-header-navbar yg-sticky-header">
                <div class="container yg-navbar-container">
                    <!-- Logo -->
                    <div class="yg-logo-container">
                        <div class="yg-site-logo">
                            <a class="yg-logo-anch" asp-controller="Home" asp-action="Index"><h2>YOGAGURU</h2></a>
                        </div>
                    </div>
                    <!-- Hamburger Menu -->
                    <div class="yg-hamburger-menu">
                        <a href="classes-page.html#" class="yg-hamburger-trigger"> <span></span> <span></span> <span></span> </a>
                    </div>
                </div>
            </div>
        </header>
        @RenderBody()
        <!-- Footer -->
        <footer>
            <div class="container yg-footer-container">
                <div class="row">
                    <div class="col-md-8 col-sm-8">
                        <ul class="yg-footer-list">
                            <li><a asp-controller="Class" asp-action="Index">Classes</a></li>
                            <li><a href="schedule.html">Schedule</a></li>
                            <li><a asp-controller="Home" asp-action="AboutUs">About Us</a></li>
                        </ul>
                    </div>
                    <div class="col-md-4 col-sm-4">
                        <ul class="yg-social-icons">
                            <li class="yg-social-icons-item">
                                <a class="yg-social-link" href="https://twitter.com/elonmusk?s=20"> <span class="yg-social-icon ti-twitter-alt"></span> </a>
                            </li>
                            <li class="yg-social-icons-item">
                                <a class="yg-social-link" href="https://www.instagram.com/cristiano/"> <span class="yg-social-icon ti-instagram"></span> </a>
                            </li>
                            <li class="yg-social-icons-item">
                                <a class="yg-social-link" href="https://www.facebook.com/WillSmith/"> <span class="yg-social-icon ti-facebook"></span> </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-12 col-sm-12">
                        <div class="yg-separator"></div>
                    </div>
                    <div class="col-md-12 col-sm-12">
                        <p class="yg-copyright">Copyright &copy; 2023. <span>Yogaguru</span> All rights reserved.</p>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/plugins/jquery-3.3.1.min.js"></script>
    <script src="~/js/plugins/bootstrap.min.js"></script>
    <script src="~/js/plugins/modernizr-2.6.2.min.js"></script>
    <script src="~/js/plugins/jquery.waypoints.min.js"></script>
    <script src="~/modules/slick/slick.js"></script>
    <script src="~/modules/magnific-popup/jquery.magnific-popup.js"></script>
    <script src="~/modules/masonry/masonry.pkgd.min.js"></script>
    <script src="~/js/script.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
